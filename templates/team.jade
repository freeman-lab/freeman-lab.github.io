extends layout

block home
	a(href='/').nav-home &lsaquo;&lsaquo;&nbsp; home

block nav
	div.nav-group
		a(href='/about/').nav.nav-about about
		span.divider /  
		a(href='/team/').nav.nav-team.nav-team-selected team
		span.divider /
		a(href='/work/').nav.nav-work work
		span.divider /
		a(href='/blog/').nav.nav-blog blog

block label
	h1.sub-header meet the team

	- var members = contents.team_members
	- var photos = contents.team.photos

	div
		- console.log(photos)
	div

	each person in members	

		- var image = _.find(photos, function(d) {return d.filepath.full.indexOf(person.metadata.id) > -1})
		
		p= person.metadata.name

		if image
			img(src='/' + image.filepath.relative).team-photo

block content
	.row
		.column-7.suffix-5.blurb
			p This is our team this is our team this is our team